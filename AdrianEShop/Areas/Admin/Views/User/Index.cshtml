@model AdrianEShop.Models.ViewModels.User.UserManagementIndexVM

<div class="row p-3 mb-3 mt-3 bg-dark">
    <div class="col-6">
        <h2 class="font-weight-light text-white">@Model.PageTitle </h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-outline-light text-white" id="create-new-item-link" asp-area="Identity" asp-page="/Account/Register">
        <i class="fas fa-plus"></i> &nbsp; Create New User</a>
    </div>
</div>

<div class="p-4 border rounded">
    <table id="table-data" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{

    <script src="~/js/Admin/User/user.js"></script>

}